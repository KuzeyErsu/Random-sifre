import random
yaz覺 = "+-/*!&$#?=@<>abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
sifre = int(input("Ne kadar uzun olsun? "))
if sifre <= 30 and sifre >= 1:
    k = ""
    for i in range(sifre):
        k += random.choice(yaz覺)
    print(k)
else:
    print("uzunluk 1 ile 30 aras覺nda olmal覺")
